RUNDEF jefeArgentino
	SELECTION ALL
	*SELECCIONA CAPITAL
	UNIVERSE PROV.PROV = 2 


  DEFINE PERSONA.JEFEARG
  AS SWITCH
  
  INCASE PERSONA.P05 = 1 AND PERSONA.P01 = 1 
  ASSIGN 1

  TYPE INTEGER
  VALUELABELS
  1 "Jefe nacido en Argentina"
  0 "Jefe nacido en otro pais"

  OPTIONS
  DEFAULT 0

  DEFINE HOGAR.JEFEARG
  AS SUM PERSONA.JEFEARG
 
  TYPE INTEGER
  VALUELABELS
  1 "Con jefe nacido en Argentina"
  

  TABLE jefeArg
  AS FREQUENCY OF HOGAR.JEFEARG
  AREABREAK RADIO


